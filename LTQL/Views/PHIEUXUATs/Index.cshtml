@model IEnumerable<LTQL.Models.PHIEUXUAT>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Phiếu xuất</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MaPX)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MaMH)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ngayxuat)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nhanvien)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.MaPX)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.MaMH)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Ngayxuat)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Nhanvien)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.MaPX }) |
        @Html.ActionLink("Details", "Details", new { id = item.MaPX }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.MaPX })
    </td>
</tr>
}

</table>
