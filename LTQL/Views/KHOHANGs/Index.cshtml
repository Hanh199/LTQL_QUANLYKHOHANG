@model IEnumerable<LTQL.Models.KHOHANG>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Kho hàng</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Makho)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tenkho)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Diachi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Thukho)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Makho)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Tenkho)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Diachi)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Thukho)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.Makho }) |
        @Html.ActionLink("Details", "Details", new { id = item.Makho }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.Makho })
    </td>
</tr>
}

</table>
